    "from mobilechelonian import Turtle\n",
    "\n",
    "# Turtle 인스턴스 생성\n",
    "t = Turtle()\n",
    "\n",
    "# 앞으로 100픽셀 이동\n",
    "t.forward(100)\n",
    "\n",
    "# 90도 회전\n",
    "t.left(90)\n",
    "\n",
    "# 다시 앞으로 100픽셀 이동\n",
    "t.forward(100)\n"
